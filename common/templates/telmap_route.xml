<?xml version="1.0" encoding="UTF-8"?>
<root:tmApiRoot
xmlns:root="http://www.telmap.com/xmlapi/v7/apiroot"
xmlns="http://www.telmap.com/xmlapi/v7/general"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
	<root:header>
		<root:bodyVersion>0</root:bodyVersion>
		<root:apiFamily>GENERAL</root:apiFamily>
	</root:header>
	<root:body xsi:type="tmRouteRequest">
		<sessionId>{{ session_id }}</sessionId>
		<origin>
			<latitude>{{ from_lat }}</latitude>
			<longitude>{{ from_lon }}</longitude>
		</origin>
		<destination>
			<latitude>{{ to_lat }}</latitude>
			<longitude>{{ to_lon }}</longitude>
		</destination>
	</root:body>
</root:tmApiRoot>
